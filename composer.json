{
    "name": "chaplean/user-bundle",
    "license": "MIT",
    "type": "symfony-bundle",
    "description": "Creates a user account and log in (from FOSUserBundle)",
    "authors": [
        {
            "name": "Chaplean",
            "homepage": "http://www.chaplean.coop"
        }
    ],
    "autoload": {
        "psr-4": {
            "Chaplean\\Bundle\\UserBundle\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Chaplean\\Bundle\\UserBundle\\": "Tests/"
        },
        "files": ["app/AppKernel.php"]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://satis.chaplean.coop"
        }
    ],
    "require": {
        "php": ">=7.0.8",
        "chaplean/mailer-bundle": "^4.0",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/jsrouting-bundle": "^1.5",
        "friendsofsymfony/rest-bundle": "^2.1",
        "friendsofsymfony/user-bundle": "^2.0.0",
        "sensio/distribution-bundle": "^5.0",
        "sensio/framework-extra-bundle": "^3.0 || ^4.0 || ^5.0",
        "symfony/assetic-bundle": "^2.3",
        "symfony/monolog-bundle": "^2.4 || ^3.0",
        "symfony/symfony": "^2.8 || ^3.0",
        "twig/extensions": "^1.0"
    },
    "require-dev": {
        "chaplean/codeship-scripts": "^1.0",
        "chaplean/coding-standard": "^1.1",
        "chaplean/unit-bundle": "^7.0",
        "mockery/mockery": "dev-master",
        "symfony/http-kernel": "^2.8 || ^3.0 || ^4.0",
        "symfony/phpunit-bridge": "^4.0",
        "symfony/var-dumper": "^2.8 || ^3.0 || ^4.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ],
        "post-update-cmd": [
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile"
        ]
    },
    "config": {
        "bin-dir": "bin",
        "discard-changes": true,
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-assets-install": "relative"
    }
}
